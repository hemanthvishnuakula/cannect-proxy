# Supabase Logging (for real-time app logs)
EXPO_PUBLIC_LOG_SUPABASE_URL=your_log_supabase_url
EXPO_PUBLIC_LOG_SUPABASE_ANON_KEY=your_log_supabase_anon_key

# PDS Invite Code (required for registration on cannect.space)
EXPO_PUBLIC_PDS_INVITE_CODE=your_invite_code_here

# Web Push (VAPID) - Get from Push VPS
EXPO_PUBLIC_VAPID_PUBLIC_KEY=your_vapid_public_key_here
EXPO_PUBLIC_PUSH_API_URL=https://push.cannect.space

# Cloudflare (optional - for image uploads)
EXPO_PUBLIC_CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
EXPO_PUBLIC_CLOUDFLARE_R2_BUCKET=cannect-media
